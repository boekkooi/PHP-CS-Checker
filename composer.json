{
  "name": "boekkooi/php-cs-fixer-checker",
  "description": "A extension for the Broadway package with extra's like tactician usage and other tweaks",
  "license" : [ "MIT" ],
  "authors": [
    {
      "name": "Warnar Boekkooi",
      "email": "warnar@boekkooi.net"
    }
  ],
  "require": {
    "php": ">=5.5",
    "fabpot/php-cs-fixer": "~2.0",
    "symfony/translation": "~2.3",
    "symfony/yaml": "~2.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Boekkooi\\CS\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Boekkooi\\CS\\": "tests/"
    }
  },
  "bin": ["php-cs-checker"],
  "minimum-stability": "dev",
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
